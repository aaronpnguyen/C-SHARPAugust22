@using Users.Models;
@model List<User>;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    
    <h1>List of users:</h1>
    @foreach(User user in Model)
    {
        <h3>@user.FirstName @user.LastName</h3>
    }
    
</body>
</html>