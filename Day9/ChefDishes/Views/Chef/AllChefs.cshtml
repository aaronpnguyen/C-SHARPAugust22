@model List<Chef>

<h1>All da hekkin chefs | <a href="/all/dishes">All da hekkin dishes</a></h1>
<a href="/add/chef">Add a Chef</a>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th># of Dishes</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Chef chef in Model)
        {   
            <tr>
                <td>@chef.FullName()</td>
                <td>@chef.Age()</td>
                <td>@chef.AllDishes.Count()</td>
            </tr>
        }
    </tbody>
</table>